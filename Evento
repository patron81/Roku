
evento1="https://raw.githubusercontent.com/patron81/Roku/refs/heads/main/Evento"
	
	

	m.port = CreateObject ("roMessagePort")
	searchRequest = CreateObject("roUrlTransfer")
	searchRequest.setURL(evento1)
	searchRequest.EnableEncodings(true)
	httpsReg = CreateObject("roRegex", "^https:", "")
	if httpsReg.isMatch(evento1)
		searchRequest.SetCertificatesFile ("common:/certs/ca-bundle.crt")
		searchRequest.AddHeader ("X-Roku-Reserved-Dev-Id", "")
		searchRequest.InitClientCertificates ()
	end if

response = searchRequest.getToString()
if response <> invalid and response <> "" 
  		texto=response
				word="var playbackURL = "
			if instr(1,texto,word)>= 0
		primero=texto.split(word)
		segundo=primero[1].split(";")
  	segundo[0] = segundo[0].Replace(chr(34), "")
		return segundo[0]
				else
				return "NADA"
   End if
    end if
